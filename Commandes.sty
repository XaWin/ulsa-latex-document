% \usepackage{lipsum}

\newcommand{\UlsaPdfSectionNew}[2]{%
    \addtocounter{section}{1}%
    \addcontentsline{toc}{section}{\protect\numberline{\thesection}#2}%
    \markboth{}{\thesection. #2}
    %\markright{#2}%
    \includepdf[frame,pages={-},width=\textwidth,pagecommand={}]{#1}%
}

\newcommand{\UlsaPdfSectionAdd}[1]{%
    \includepdf[frame,pages={-},width=\textwidth,pagecommand={}]{#1}%
}